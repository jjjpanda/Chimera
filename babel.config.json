    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        ["import", {"libraryName": "antd-mobile", "style": true}, "antd-mobile" ],
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-runtime"
    ],
    "only": [
        "./frontend",
        "./backend",
        "./dist"
    ]
}