{
  "name": "chimera",
  "version": "4.0.0",
  "description": "playback",
  "main": "server.js",
  "scripts": {
    "prestart": "node register.js && npx mkdirp ./.well-known/acme-challenge && cd command && npm run build",
    "start": "npx pm2 start pm2prod.config.js",
    "server": "node server.js",
    "list": "npx pm2 list",
    "monitor": "npx pm2 monit",
    "kill": "npx pm2 stop all",
    "delete": "npx pm2 delete all",
    "stop": "npm run kill && npm run delete && pkill PM2",
    "logs": "npx pm2 logs",
    "lint": "eslint --ext .jsx --ext .js \".\" -o eslint/lintOutput.html -f html",
    "lintfix": "eslint --ext .jsx --ext .js \".\" -o eslint/lintOutput.html -f html --fix",
    "command_install": "cd command && npm install",
    "livestream_install": "cd livestream && npm install",
    "schedule_install": "cd schedule && npm install",
    "storage_install": "cd storage && npm install",
    "lib_install": "cd lib && npm install",
    "gateway_install": "cd gateway && npm install",
    "memory_install": "cd memory && npm install",
    "postinstall": "npm run command_install && npm run livestream_install && npm run schedule_install && npm run storage_install && npm run lib_install && npm run gateway_install && npm run memory_install",
    "startCont": "npx pm2 start pm2dev.config.js",
    "buildCont": "cd command && npm run buildCont"
  },
  "dependencies": {
    "@tensorflow-models/coco-ssd": "^2.2.2",
    "@tensorflow/tfjs-converter": "^3.11.0",
    "@tensorflow/tfjs-core": "^3.11.0",
    "@tensorflow/tfjs-node": "3.3.0",
    "command": "file:./command",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "gateway": "file:./gateway",
    "heartbeat": "github:jjjpanda/heartbeat",
    "helmet": "^4.6.0",
    "lib": "file:./lib",
    "livestream": "file:./livestream",
    "memory": "file:./memory",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.1",
    "node-cron": "^2.0.3",
    "pm2": "^5.1.2",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "schedule": "file:./schedule",
    "storage": "file:./storage"
  },
  "devDependencies": {
    "eslint": "^8.2.0",
    "eslint-plugin-react": "^7.27.0"
  }
}
